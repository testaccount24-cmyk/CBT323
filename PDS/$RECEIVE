//SBGOLOBL JOB (ACCT#),RECEIVE,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//* --------------------------------------------------- *//
//*      RECEIVE JOB TO CREATE INSTALL LIBRARIES        *//
//*                                                     *//
//*     (THIS JOB WAS TESTED TO WORK ON MY SYSTEM)      *//
//* --------------------------------------------------- *//
//TSOBATCH EXEC PGM=IKJEFT01
//SYSTSPRT DD SYSOUT=*
//SYSTSIN DD *
   RECEIVE INDS('SBGOLOB.CBT477.FILE323(LOAD)')
   DSN ('SBGOLOB.FILE323.LOAD') VOL(WORK04)
   RECEIVE INDS('SBGOLOB.CBT477.FILE323(MACLIB)')
   DSN ('SBGOLOB.FILE323.MACLIB') VOL(WORK04)
   RECEIVE INDS('SBGOLOB.CBT477.FILE323(SAMPLIB)')
   DSN ('SBGOLOB.FILE323.SAMPLIB') VOL(WORK04)
   RECEIVE INDS('SBGOLOB.CBT477.FILE323(SRCLIB)')
   DSN ('SBGOLOB.FILE323.SRCLIB') VOL(WORK04)
