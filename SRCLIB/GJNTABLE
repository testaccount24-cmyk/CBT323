GJNTABLE CSECT
GJNTABLE AMODE 31
GJNTABLE RMODE ANY
.*
.**********************************************************************
.*                                                                    *
.* Component_Type: MVS HL-Assembler Module                            *
.*    Description: TR & TRT Tables for various macros                 *
.*         Author: Gerard J Nicol                                     *
.*           Date: September 1997                                     *
.*                                                                    *
.*--+----1----+----2----+----3----+----4----+----5----+----6----+----7* --+----8
.*
         ENTRY $VBIN
         ENTRY $VALPHA
         ENTRY $VNUM
         ENTRY $VSPEC
         ENTRY $VWORK
         ENTRY $VINIT
         ENTRY $H2C
         ENTRY $S2Z
         ENTRY $Z2S
         ENTRY $C2H
         ENTRY $C2X
         ENTRY $X2C
         USING *,15
*
$VINIT   EQU  *
         DC   256XL1'01'
$VWORK   EQU  *
         DC   256XL1'00'
$VALPHA  EQU  *
         DC   256XL1'01'
         ORG  $VALPHA+C'a'
         DC   9XL1'00'
         ORG  $VALPHA+C'j'
         DC   9XL1'00'
         ORG  $VALPHA+C's'
         DC   8XL1'00'
         ORG  $VALPHA+C'A'
         DC   9XL1'00'
         ORG  $VALPHA+C'J'
         DC   9XL1'00'
         ORG  $VALPHA+C'S'
         DC   8XL1'00'
         ORG  $VALPHA+256
*
$VNUM    EQU  *
         DC   256XL1'01'
         ORG  $VNUM+C'0'
         DC   10XL1'00'
         ORG  $VNUM+256
*
$VBIN    EQU  *
         DC   256XL1'01'
         ORG  $VBIN+C'0'
         DC   2XL1'00'
         ORG  $VBIN+256
*
$VSPEC   EQU  *
         DC   256XL1'01'
         ORG  $VSPEC+X'4A'              -¢.<(+|&
         DC   7XL1'00'
         ORG  $VSPEC+X'5A'              -!$*);¬-/
         DC   8XL1'00'
         ORG  $VSPEC+X'6A'              -¦,%_>?
         DC   6XL1'00'
         ORG  $VSPEC+X'79'              -`:#@'="
         DC   7XL1'00'
         ORG  $VSPEC+X'A1'              -~
         DC   1XL1'00'
         ORG  $VSPEC+X'C0'              -{
         DC   1XL1'00'
         ORG  $VSPEC+X'D0'              -}
         DC   1XL1'00'
         ORG  $VSPEC+X'E0'              -\
         DC   1XL1'00'
         ORG  $VSPEC+X'40'              -Space
         DC   1XL1'00'
         ORG  $VSPEC+256
*
$C2X     EQU  *
$H2C     DC   C'0123456789ABCDEF'
*
$S2Z     DC   XL16'000102030405060708090A0B0C0D0E0F'
         DC   XL16'101112131415161718191A1B1C1D1E1F'
         DC   XL16'202122232425262728292A2B2C2D2E2F'
         DC   XL16'303132333435363738393A3B3C3D3E3F'
         DC   XL16'F04142434445464748494A4B4C4D4E4F'
         DC   XL16'505152535455565758595A5B5C5D5E5F'
         DC   XL16'606162636465666768696A6B6C6D6E6F'
         DC   XL16'707172737475767778797A7B7C7D7E7F'
         DC   XL16'808182838485868788898A8B8C8D8E8F'
         DC   XL16'909192939495969798999A9B9C9D9E9F'
         DC   XL16'A0A1A2A3A4A5A6A7A8A9AAABACADAEAF'
         DC   XL16'B0B1B2B3B4B5B6B7B8B9BABBBCBDBEBF'
         DC   XL16'C0C1C2C3C4C5C6C7C8C9CACBCCCDCECF'
         DC   XL16'D0D1D2D3D4D5D6D7D8D9DADBDCDDDEDF'
         DC   XL16'E0E1E2E3E4E5E6E7E8E9EAEBECEDEEEF'
         DC   XL16'F0F1F2F3F4F5F6F7F8F9FAFBFCFDFEFF'
*
$Z2S     DC   256XL1'00'
         ORG  $Z2S+C'1'
         DC   XL9'FFFFFFFFFFFFFFFFFF'
         ORG  $Z2S+256
         DC   XL16'000102030405060708090A0B0C0D0E0F'
         DC   XL16'101112131415161718191A1B1C1D1E1F'
         DC   XL16'202122232425262728292A2B2C2D2E2F'
         DC   XL16'303132333435363738393A3B3C3D3E3F'
         DC   XL16'404142434445464748494A4B4C4D4E4F'
         DC   XL16'505152535455565758595A5B5C5D5E5F'
         DC   XL16'606162636465666768696A6B6C6D6E6F'
         DC   XL16'707172737475767778797A7B7C7D7E7F'
         DC   XL16'808182838485868788898A8B8C8D8E8F'
         DC   XL16'909192939495969798999A9B9C9D9E9F'
         DC   XL16'A0A1A2A3A4A5A6A7A8A9AAABACADAEAF'
         DC   XL16'B0B1B2B3B4B5B6B7B8B9BABBBCBDBEBF'
         DC   XL16'C0C1C2C3C4C5C6C7C8C9CACBCCCDCECF'
         DC   XL16'D0D1D2D3D4D5D6D7D8D9DADBDCDDDEDF'
         DC   XL16'E0E1E2E3E4E5E6E7E8E9EAEBECEDEEEF'
         DC   XL16'40F1F2F3F4F5F6F7F8F9FAFBFCFDFEFF'
*
$X2C     EQU  *
$C2H     DC   XL16'000102030405060708090A0B0C0D0E0F'
         DC   XL16'101112131415161718191A1B1C1D1E1F'
         DC   XL16'202122232425262728292A2B2C2D2E2F'
         DC   XL16'303132333435363738393A3B3C3D3E3F'
         DC   XL16'404142434445464748494A4B4C4D4E4F'
         DC   XL16'505152535455565758595A5B5C5D5E5F'
         DC   XL16'606162636465666768696A6B6C6D6E6F'
         DC   XL16'707172737475767778797A7B7C7D7E7F'
         DC   XL16'808182838485868788898A8B8C8D8E8F'
         DC   XL16'909192939495969798999A9B9C9D9E9F'
         DC   XL16'A0A1A2A3A4A5A6A7A8A9AAABACADAEAF'
         DC   XL16'B0B1B2B3B4B5B6B7B8B9BABBBCBDBEBF'
         DC   XL16'C00A0B0C0D0E0FC7C8C9CACBCCCDCECF'
         DC   XL16'D0D1D2D3D4D5D6D7D8D9DADBDCDDDEDF'
         DC   XL16'E0E1E2E3E4E5E6E7E8E9EAEBECEDEEEF'
         DC   XL16'00010203040506070809FAFBFCFDFEFF'
*
         END  GJNTABLE
